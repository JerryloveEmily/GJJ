        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.BaseActivity" />
        <activity
            android:name=".view.activity.MainActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--我的公积金-->
        <activity
            android:name=".view.activity.MyGjjActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" />
        <!--办事指南-->
        <activity
            android:name=".view.activity.GuideActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" />
        <!--办事流程-->
        <activity
            android:name=".view.activity.FlowActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" />
        <!--提取材料-->
        <activity
            android:name=".view.activity.MaterialActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" />
        <!--本地政策-->
        <activity
            android:name=".view.activity.LocalPolicyActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" />
        <!--在线问答-->
        <activity
            android:name=".view.activity.OnlineQuestionsActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" />
    </application>

</manifest>
